{
    "name": "vue-ele-form",
    "description": "基于 element-ui 的数据驱动表单组件",
    "version": "0.3.19",
    "license": "MIT",
    "private": false,
    "scripts": {
        "serve": "vue-cli-service serve",
        "build:lib": "vue build -t lib --name vue-ele-form -d ./dist/ ./lib/index.js",
        "build": "npm run lint & npm run build:lib",
        "lint": "vue-cli-service lint"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/dream2023/vue-ele-form"
    },
    "main": "lib/index.js",
    "homepage": "https://github.com/dream2023/vue-ele-form",
    "keywords": [
        "vue-ele-form",
        "vue form",
        "element form",
        "element",
        "element-ui form",
        "el-form"
    ],
    "dependencies": {
        "dayjs": "^1.8.15",
        "lodash.clonedeep": "^4.5.0",
        "vue-ele-gallery": "^2.1.3"
    },
    "devDependencies": {
        "@vue/cli-plugin-babel": "^3.8.0",
        "@vue/cli-plugin-eslint": "^3.8.0",
        "@vue/cli-service": "^3.8.0",
        "@vue/eslint-config-standard": "^4.0.0",
        "axios": "^0.19.0",
        "babel-eslint": "^10.0.1",
        "core-js": "^2.6.5",
        "element-ui": "^2.11.1",
        "eslint": "^5.16.0",
        "eslint-plugin-vue": "^5.0.0",
        "lint-staged": "^8.1.5",
        "vue": "^2.6.10",
        "vue-template-compiler": "^2.6.10"
    },
    "gitHooks": {
        "pre-commit": "lint-staged"
    },
    "lint-staged": {
        "src/*.{js,vue}": [
            "vue-cli-service lint",
            "git add"
        ]
    }
}
